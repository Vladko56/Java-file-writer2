import java.io.FileWriter;
import java.io.IOException;

	public class Main {
		public static void main(String[] args) {
			
			try {
				FileWriter writer = new FileWriter("example.txt");
				writer.write("input what you want here");
				writer.write("\n(input what you want here");
				writer.close();
		}
			catch (IOException e) {
				e.printStackTrace();
			}
		}
	}

//written and tested in Eclipse IDE
